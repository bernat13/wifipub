{% extends 'AWifiPubBundle::layout.html.twig' %}

 {% block contenido %}
<h2>Modificar Instalacion</h2>

{% if mensaje is defined %}
<b><span style="color: red;">{{ mensaje }}</span></b>
{% endif %}
<br/>


<form name="formInsertar" action="{{ path('wifipub_admin_insalacion_set', {'id': id})}}" method="POST">
    <table>
      <tr>
        <td>
          Descripcion
        </td>     
        <td>
          <input type="text" name="Descripcion" value="{{ Descripcion }}" />
        </td> 
      </tr>   
      <tr>
        <td>
          Direccion
        </td>     
        <td>
          <input type="text" name="Direccion" value="{{ Direccion }}" />
        </td> 
      </tr>   
      <tr>
        <td>
          ContactoNombre
        </td>     
        <td>
          <input type="text" name="ContactoNombre" value="{{ ContactoNombre }}" />
        </td> 
      </tr>   
      <tr>
        <td>
          Email
        </td>     
        <td>
          <input type="text" name="Email" value="{{ Email }}" />
        </td> 
      </tr>   
      <tr>
        <td>
          Telf
        </td>     
        <td>
          <input type="text" name="Telf" value="{{ Telf }}" />
        </td> 
      </tr>   
      <tr>
        <td>
          PosiciónGPS
        </td>     
        <td>
          <input type="text" name="PosiciónGPS" value="{{ PosiciónGPS }}" />
        </td> 
      </tr>   
      <tr>
        <td>
          TipoInstalacion
        </td>     
        <td>
            <input type="text" name="TipoInstalacion" value="{{ TipoInstalacion }}" />
        </td> 
      </tr>   
      <tr>
        <td>
          TipoAccesoInternet
        </td>     
        <td>
           <input type="text" name="TipoAccesoInternet" value="{{ TipoAccesoInternet }}" />
        </td> 
      </tr>   
      <tr>
        <td>
          DownMbps
        </td>     
        <td>
          <input type="text" name="DownMbps" value="{{ DownMbps }}" />
         
        </td> 
      </tr>   
      <tr>
        <td>
          UpMbps
        </td>     
        <td>
          <input type="text" name="UpMbps" value="{{ UpMbps }}" />
        </td> 
      </tr>   
     
    </table>
    <input type="submit" value="Modificar" name="Modificar" />
</form>

{% endblock %}