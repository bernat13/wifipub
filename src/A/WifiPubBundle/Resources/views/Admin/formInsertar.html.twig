{% extends 'AWifiPubBundle::layout.html.twig' %}

 {% block contenido %}
<h2>Insertar {{tabla}}</h2>

{% if mensaje is defined %}
<b><span style="color: red;">{{ mensaje }}</span></b>
{% endif %}
<br/>

<form name="formInsertar" action="{{ path('wifipub_admin_add', {'tabla': tabla})}}" method="POST">
    <table>
 
        <tr>

        <td>Nombre</td>     <td><input type="text" name="Nombre" value="{{ Nombre }}" /></td> </tr>   <tr>
         <td>Apellido </td>    <td><input type="text" name="Apellido" value="{{ Apellido }}" /></td> </tr>   <tr>
                  <td>razonComercial</td>    <td><input type="text" name="razonComercial" value="{{ razonComercial }}" /></td> </tr>   <tr>
          <td>Direccion</td>   <td><input type="text" name="Direccion" value="{{ Direccion }}" /></td> </tr>   <tr>
             <td>ContactoNombre</td>  <td><input type="text" name="ContactoNombre" value="{{ ContactoNombre }}" /></td> </tr>   <tr>
          <td>Email</td>   <td><input type="text" name="Email" value="{{ Email }}" /></td> </tr>   <tr>
        <td>Telf</td>    <td><input type="text" name="Telf" value="{{ Telf }}" /></td> </tr>   <tr>
            <td>CIF</td>   <td><input type="text" name="CIF" value="{{ CIF }}" /></td>  </tr>   

    </table>
    <input type="submit" value="insertar" name="insertar" />
</form>

{% endblock %}